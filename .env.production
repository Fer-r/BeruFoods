# Production Environment Configuration for AWS EC2
# Replace values with your actual production credentials

# MySQL Database Configuration
MYSQL_DATABASE=berufoods_prod
MYSQL_USER=berufoods_user
MYSQL_PASSWORD=CHANGE_ME_STRONG_PASSWORD_123!
MYSQL_ROOT_PASSWORD=CHANGE_ME_ROOT_PASSWORD_456!

# Symfony Configuration
APP_ENV=prod
APP_SECRET=CHANGE_ME_APP_SECRET_RANDOM_STRING_789!

# JWT Configuration
JWT_PASSPHRASE=CHANGE_ME_JWT_PASSPHRASE_ABC!

# Mercure Configuration (HMAC secret for HS256 algorithm)
MERCURE_JWT_SECRET=CHANGE_ME_MERCURE_JWT_SECRET_DEF!
MERCURE_SECRET=CHANGE_ME_MERCURE_SECRET_GHI!

# Public URLs for production (will be auto-configured during deployment)
MERCURE_PUBLIC_URL=https://localhost/.well-known/mercure
MERCURE_URL=http://mercure:3000/.well-known/mercure

# CORS Configuration for production (will be auto-configured during deployment)
CORS_ALLOW_ORIGIN=^https?://(localhost|127\.0\.0\.1)(:[0-9]+)?$

# API Configuration for frontend (will be auto-configured during deployment)
VITE_URL_API=https://localhost/api

# Google Maps API Configuration
VITE_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Production database server version
DB_SERVER_VERSION=8.0 